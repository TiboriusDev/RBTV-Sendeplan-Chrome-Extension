{
  "name": "RBTV Sendeplan",
  "version": "1.8",
  "description": "Zeigt den Sende- und Uploadplan von Rocketbeans.tv",
  "permissions": ["storage","activeTab", "tabs", "<all_urls>"],
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "images/logo.webp"
    },
  "icons": {
    "16": "images/logo.webp",
    "48": "images/logo.webp",
    "128": "images/logo.webp"
  }
}